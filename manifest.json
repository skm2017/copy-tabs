{
    "manifest_version": 2,
    "name": "Copy Tabs",
    "description": "With this extension you can copy url of all open tabs in your browser.",
    "version": "1.2.1",
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/copy_tabs16.png",
            "32": "images/copy_tabs32.png",
            "48": "images/copy_tabs48.png",
            "128": "images/copy_tabs128.png"
        }
    },
    "icons": {
        "16": "images/copy_tabs16.png",
        "32": "images/copy_tabs32.png",
        "48": "images/copy_tabs48.png",
        "128": "images/copy_tabs128.png"
    }
}